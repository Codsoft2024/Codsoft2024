class A{
public void m1(){
System.out.println("m1 of A class");
}
}
class B extends A{ 
public void m1(){
System.out.println("m1 of B class");
}
public void m2(){
System.out.println("m2 of B class");
}
}
public class DMD{
static public void main(string ...a){
 A c = new A();
 a.m1();
B b = new B();
b.m1();
b.m2();
c = b;
c.m1();
}
}
