Abstract class P{
abstract public void dis();
public void getname(){
System.out.println("***"+this.getClass());
}
}
class C1 extends P{
public void disp(){
System.out.println("#######Def.by C1"):
}
}
class C2 extends P{
public void disp(){
System.out.println("Def.by C2*********"):
}
}
public class AbstractDemo{
static public void main(String ...args){
P o1; //= new P();
  o1 = new C1();
  o1.disp();
  o1 = new C2();
  o1.disp();
}
}
